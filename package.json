{
  "name": "pcf-project",
  "version": "1.0.0",
  "description": "Project containing your PowerApps Component Framework (PCF) control.",
  "scripts": {
    "build": "pcf-scripts build",
    "clean": "pcf-scripts clean",
    "rebuild": "pcf-scripts rebuild",
    "start": "pcf-scripts start",
    "refreshTypes": "pcf-scripts refreshTypes",
    "watch": "chokidar 'src/**/*.{ts,tsx,css}' --debounce 500 --throttle 500 --ignore-initial -c 'npm run rebuild'",
    "dev": "concurrently \"npm run start\" \"npm run watch\""
  },
  "dependencies": {
    "@fluentui/react": "^8.112.4",
    "@fluentui/react-icons-mdl2": "^1.3.50",
    "@types/css-modules": "^1.0.4",
    "axios": "^1.6.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@types/node": "^16.4",
    "@types/powerapps-component-framework": "^1.3.0",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "@typescript-eslint/eslint-plugin": "^4.29.0",
    "@typescript-eslint/parser": "^4.29.0",
    "chokidar-cli": "^3.0.0",
    "concurrently": "^8.2.2",
    "css-loader": "^6.8.1",
    "eslint": "^7.32.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.0",
    "nodemon": "^3.0.1",
    "pcf-scripts": "^1",
    "pcf-start": "^1",
    "style-loader": "^3.3.3",
    "typescript": "^4.3"
  }
}
